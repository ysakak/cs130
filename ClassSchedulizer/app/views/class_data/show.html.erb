<div class="row col-md-10 col-md-offset-1">
  <div id="class_details">
    <div class="row">
      <table class="table">
        <tr>
          <th colspan=6><%= @class_data.course_id %> - <%= @class_data.title %></th>
        </tr>
        <% if false %>
        <tr>
          <th>Day(s)</th>
          <th>Start Time</th>
          <th>End Time</th>
          <th>Instructor(s)</th>
          <th>Bruinwalk Rating</th>
          <th>Details</th>
        </tr>
        <tr>
          <td><%= @class_data.days %></td>
          <td><%= @class_data.start_time.in_time_zone('Pacific Time (US & Canada)').strftime("%H:%M %P") %></td>
          <td><%= @class_data.end_time.in_time_zone('Pacific Time (US & Canada)').strftime("%H:%M %P") %></td>
          <td><%= @class_data.instructor %></td>
          <td>Bruinwalk Rating</td>
          <td>Details</td>
        </tr>
        <% end %>
        <tr>
          <th colspan=6>Description</th>
        </tr>
        <tr>
          <td colspan=6><%= @class_data.description %></td>
        </tr>
        <tr>
          <th colspan=6>Enrollment Information</th>
        </tr>
        <tr>
          <th>Units</th>
          <th>Grade Type</th>
          <th>Restrictions</th>
          <th>Impacted</th>
          <th>Level</th>
          <th></th>
        </tr>
        <tr>
          <td><%= @class_data.units %></td>
          <td><%= @class_data.grade_type %></td>
          <td><%= @class_data.restrictions %></td>
          <td><%= @class_data.impacted_class %></td>
          <td><%= @class_data.level %></td>
          <td></td>
        </tr>
      </table>
    </div>
  </div>
</div>
