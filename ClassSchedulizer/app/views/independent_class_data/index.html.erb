<p id="notice"><%= notice %></p>

<h1>Independent Class Data</h1>

<table>
  <thead>
    <tr>
      <th>Lecture</th>
      <th>Class type</th>
      <th>Section</th>
      <th>Course</th>
      <th>Title</th>
      <th>Major</th>
      <th>Major code</th>
      <th>Term</th>
      <th>Description</th>
      <th>Days</th>
      <th>Start time</th>
      <th>End time</th>
      <th>Location</th>
      <th>Units</th>
      <th>Instructor</th>
      <th>Final examination date</th>
      <th>Final examination day</th>
      <th>Final examination time</th>
      <th>Grade type</th>
      <th>Restrictions</th>
      <th>Impacted class</th>
      <th>Level</th>
      <th>Text book url</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @independent_class_data.each do |independent_class_data| %>
      <tr>
        <td><%= independent_class_data.lecture_id %></td>
        <td><%= independent_class_data.class_type %></td>
        <td><%= independent_class_data.section %></td>
        <td><%= independent_class_data.course_id %></td>
        <td><%= independent_class_data.title %></td>
        <td><%= independent_class_data.major %></td>
        <td><%= independent_class_data.major_code %></td>
        <td><%= independent_class_data.term %></td>
        <td><%= independent_class_data.description %></td>
        <td><%= independent_class_data.days %></td>
        <td><%= independent_class_data.start_time %></td>
        <td><%= independent_class_data.end_time %></td>
        <td><%= independent_class_data.location %></td>
        <td><%= independent_class_data.units %></td>
        <td><%= independent_class_data.instructor %></td>
        <td><%= independent_class_data.final_examination_date %></td>
        <td><%= independent_class_data.final_examination_day %></td>
        <td><%= independent_class_data.final_examination_time %></td>
        <td><%= independent_class_data.grade_type %></td>
        <td><%= independent_class_data.restrictions %></td>
        <td><%= independent_class_data.impacted_class %></td>
        <td><%= independent_class_data.level %></td>
        <td><%= independent_class_data.text_book_url %></td>
        <td><%= independent_class_data.url %></td>
        <td><%= link_to 'Show', independent_class_data %></td>
        <td><%= link_to 'Edit', edit_independent_class_data_path(independent_class_data) %></td>
        <td><%= link_to 'Destroy', independent_class_data, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Independent Class Datum', new_independent_class_data_path %>
