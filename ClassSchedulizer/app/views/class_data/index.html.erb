<p id="notice"><%= notice %></p>

<h1>Class Data</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @class_data.each do |class_data| %>
      <tr>
        <td><%= link_to 'Show', class_data %></td>
        <td><%= link_to 'Edit', edit_class_data_path(class_data) %></td>
        <td><%= link_to 'Destroy', class_data, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Class Data', new_class_data_path %>
